  "readme": "https://github.com/Azure/perks.extension/readme.md",
  "devDependencies": {
    "@types/npm": "^2.0.28",
    "mocha-typescript": "1.1.17",
    "@types/node": "10.11.3",
    "@types/mocha": "5.2.5",
    "mocha": "5.2.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@microsoft.azure/async-io": "2.0.22",
    "@microsoft.azure/eventing": "2.0.16",
    "@microsoft.azure/tasks": "2.0.28",
